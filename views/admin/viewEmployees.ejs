<!DOCTYPE html>
<html>
<head>
	<title>All Employee List</title>
	<link rel="stylesheet" type="text/css" href="abc/style.css">
</head>
<body>
	
	<a href="/admin">Back</a> | 
  <a href="/logout">Logout</a>
	
	<center>
		<h3>All Employees</h3>
		<table border="1" style="width:800px">
			<tr>
				<th>NAME</th>
				<th>CONTACT NO.</th>
				<th>USERNAME</th>		
				<th>PASSWORD</th>
				<th>ACTION</th>
			</tr>
      <%for(var i=0; i<list.length; i++) { %>
			<tr>
				<th><%= list[i].name %></th>
				<th><%= list[i].phone %></th>
				<th><%= list[i].username %></th>
        <th><%= list[i].password %></th>
				<th><a href="/admin/update">UPDATE</a>| <a href="/admin/delete">DELETE</a></th>
			</tr>
      <% } %>
		</table>
	</center>
</body>
</html>